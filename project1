t = int(input('Введите t:'))


def circle_move(phi, time,delay):
    if delay<time:
      x=0
      y=0
    else:
      if t >= 90:
        a = 0.00001 * 70 * t
      else:
        a = 0.00001 * t
      vy0 = 0.02 * t
      y0 = vy0 * (time-delay)
      alpha = a * (np.pi / 180) * (time-delay)
      R = alpha * (time-delay)
      x=R*np.cos(phi)
      y=y0+R*np.sin(phi)
    return x, y


def circle_move1(phi, time, delay):
    if delay<time:
      x=0
      y=0
    else:
      if t >= 90:
          a = 0.00001 * 70 * t
      else:
          a = 0.00001 * t
      vy0 = 0.02 * t
      y0 = vy0 * (time-delay)
      alpha = a * (np.pi / 180) * (time-delay)
      R = alpha
      x = R * np.cos(phi) + 30
      y = y0 + R * np.sin(phi)
    return x, y


